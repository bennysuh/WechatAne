<extension xmlns="http://ns.adobe.com/air/extension/3.8">
	<id>com.weixin.ane</id>
	<versionNumber>1</versionNumber>
	<platforms>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libWeixinSdk.a</nativeLibrary>
				<initializer>WeixinSdkExtInitializer</initializer>
				<finalizer>WeixinSdkExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-ARM">
		  <applicationDeployment>
			<nativeLibrary>weixinsdk.jar</nativeLibrary>
			<initializer>com.weixin.ane.WeixinExtension</initializer>
			<finalizer>com.weixin.ane.WeixinExtension</finalizer>
		  </applicationDeployment>
		</platform>
		<platform name="default"> 
			<applicationDeployment/> 
		</platform>
	</platforms>
</extension>
